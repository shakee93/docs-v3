{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "outputDirectory": "public",
 "rewrites": [
    {
      "source": "/v2",
      "destination": "https://rapidload-docs.vercel.app"
    },
    {
      "source": "/v2/_nuxt/:path*",
      "destination": "https://rapidload-docs.vercel.app/_nuxt/:path*"
    },
    {
      "source": "/v2/:path*",
      "destination": "https://rapidload-docs.vercel.app/:path*"
    }
  ]

}


